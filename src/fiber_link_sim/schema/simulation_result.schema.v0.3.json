          "type": "string"
        },
        "message": {
          "title": "Message",
          "type": "string"
        },
        "details": {
          "additionalProperties": true,
          "title": "Details",
          "type": "object"
        }
      },
      "required": [
        "code",
        "message"
      ],
      "title": "ErrorInfo",
      "type": "object"
    },
    "Errors": {
      "additionalProperties": false,
      "properties": {
        "pre_fec_ber": {
          "minimum": 0,
