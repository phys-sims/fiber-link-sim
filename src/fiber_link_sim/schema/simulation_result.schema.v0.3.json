      "additionalProperties": false,
      "properties": {
        "raw_line_rate": {
          "minimum": 0,
          "title": "Raw Line Rate",
          "type": "number"
        },
        "net_after_fec": {
          "minimum": 0,
          "title": "Net After Fec",
          "type": "number"
        },
        "goodput_est": {
          "minimum": 0,
          "title": "Goodput Est",
          "type": "number"
        }
