        "type": {
          "enum": [
            "png",
            "npz",
            "json",
            "txt",
            "bin"
          ],
          "title": "Type",
          "type": "string"
        },
        "ref": {
          "title": "Ref",
          "type": "string"
        },
        "mime": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Mime"
        },
        "bytes": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Bytes"
        }
      },
      "required": [
        "name",
        "type",
        "ref"
      ],
      "title": "Artifact",
      "type": "object"
    },
    "ErrorInfo": {
      "additionalProperties": false,
