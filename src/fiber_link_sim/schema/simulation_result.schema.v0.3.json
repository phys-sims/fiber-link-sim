  "additionalProperties": false,
  "properties": {
    "v": {
      "minLength": 1,
      "title": "V",
      "type": "string"
    },
    "status": {
      "enum": [
        "success",
        "error"
      ],
      "title": "Status",
      "type": "string"
    },
    "summary": {
      "anyOf": [
        {
          "$ref": "#/$defs/Summary"
        },
